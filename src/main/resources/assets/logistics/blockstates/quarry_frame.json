{
  "multipart": [
    {
      "apply": { "model": "logistics:block/quarry_frame_core" }
    },
    {
      "when": { "north": "true" },
      "apply": { "model": "logistics:block/quarry_frame_arm" }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "logistics:block/quarry_frame_arm", "y": 180 }
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "logistics:block/quarry_frame_arm", "y": 90 }
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "logistics:block/quarry_frame_arm", "y": 270 }
    },
    {
      "when": { "up": "true" },
      "apply": { "model": "logistics:block/quarry_frame_arm", "x": 270 }
    },
    {
      "when": { "down": "true" },
      "apply": { "model": "logistics:block/quarry_frame_arm", "x": 90 }
    }
  ]
}
