{
  "multipart": [
    {
      "apply": {
        "model": "logistics:block/pipe_core"
      }
    },
    {
      "when": {
        "north": "pipe"
      },
      "apply": {
        "model": "logistics:block/pipe_side"
      }
    },
    {
      "when": {
        "south": "pipe"
      },
      "apply": {
        "model": "logistics:block/pipe_side",
        "y": 180
      }
    },
    {
      "when": {
        "east": "pipe"
      },
      "apply": {
        "model": "logistics:block/pipe_side",
        "y": 90
      }
    },
    {
      "when": {
        "west": "pipe"
      },
      "apply": {
        "model": "logistics:block/pipe_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "pipe"
      },
      "apply": {
        "model": "logistics:block/pipe_side_up"
      }
    },
    {
      "when": {
        "down": "pipe"
      },
      "apply": {
        "model": "logistics:block/pipe_side_down"
      }
    }
  ]
}
