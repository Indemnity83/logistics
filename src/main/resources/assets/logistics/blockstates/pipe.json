{
  "multipart": [
    {
      "apply": {
        "model": "logistics:block/pipe_core"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "logistics:block/pipe_side"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "logistics:block/pipe_side",
        "y": 180
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "logistics:block/pipe_side",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "logistics:block/pipe_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "logistics:block/pipe_side_up"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "logistics:block/pipe_side_down"
      }
    }
  ]
}
